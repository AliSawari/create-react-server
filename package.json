{
  "name": "create-react-server",
  "version": "0.1.0",
  "description": "Middleware for React + Router + Redux with Server Side Rendering",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf ./build/*",
    "build": "npm run clean && NODE_ENV=production webpack --progress",
    "start": "NODE_ENV=development babel-node ./server.js",
    "server": "NODE_ENV=production babel-node ./server.js"
  },
  "author": "Kirill Konshin",
  "devDependencies": {
    "express": "^4.14.1",
    "jest": "^19.0.2",
    "node-fetch": "^1.6.3"
  },
  "dependencies": {
    "hoist-non-react-statics": "^1.2.0",
    "memory-fs": "^0.4.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "react-router": "3.*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kirill-konshin/rcreate-react-server.git"
  },
  "bugs": {
    "url": "https://github.com/kirill-konshin/create-react-server/issues"
  },
  "homepage": "https://github.com/kirill-konshin/create-react-server",
  "engines": {
    "node": ">=0.10.36"
  },
  "license": "MIT",
  "jest": {
    "testRegex": "(/src/.*\\.spec.js)$",
    "collectCoverage": true
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "dynamic-import-webpack",
      "transform-ensure-ignore"
    ]
  }
}

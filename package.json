{
  "name": "react-router-redux-middleware",
  "version": "1.1.0",
  "description": "Middleware for React + Router + Redux with Server Side Rendering",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf ./build/*",
    "build": "npm run clean && NODE_ENV=production webpack --progress",
    "start": "NODE_ENV=development babel-node ./server.js",
    "server": "NODE_ENV=production babel-node ./server.js"
  },
  "author": "Kirill Konshin",
  "devDependencies": {
    "@webpack-blocks/babel6": "^0.4.1",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-cli": "^6.23.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-ensure-ignore": "^0.1.0",
    "babel-preset-react-app": "^2.1.1",
    "express": "^4.14.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "node-fetch": "^1.6.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "striptags": "^3.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "memory-fs": "^0.4.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "react-router": "3.*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kirill-konshin/react-router-redux-middleware.git"
  },
  "bugs": {
    "url": "https://github.com/kirill-konshin/react-router-redux-middleware/issues"
  },
  "homepage": "https://github.com/kirill-konshin/react-router-redux-middleware",
  "engines": {
    "node": ">=0.10.36"
  },
  "license": "MIT",
  "jest": {
    "testRegex": "(/src/.*\\.spec.js)$",
    "collectCoverage": true
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "dynamic-import-webpack",
      "transform-ensure-ignore"
    ]
  }
}
